{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://classroom-reservation-backend-fhb6gjdjgrgbh0fz.westus-01.azurewebsites.net/api/*"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/static/*", "/images/*", "/css/*", "/js/*"]
  }
}
